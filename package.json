{
  "name": "snappy-js",
  "version": "1.0.0",
  "description": "Native Node.js bindings for Google's Snappy compression library with precompiled binaries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist build",
    "prebuild": "prebuild --all --strip --verbose",
    "build": "npm run build:ts && npm run build:native",
    "build:ts": "tsc",
    "build:native": "node-gyp rebuild",
    "install": "prebuild-install || npm run build:native",
    "test": "npm run build:ts && node test/test.js",
    "example": "npm run build:ts && node examples/basic-usage.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "snappy",
    "compression",
    "native",
    "addon",
    "compress",
    "decompress",
    "fast",
    "performance",
    "google",
    "bindings",
    "typescript"
  ],
  "author": "Thiago Santos",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/snappy-js.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/snappy-js/issues"
  },
  "homepage": "https://github.com/seu-usuario/snappy-js#readme",
  "gypfile": true,
  "binary": {
    "module_name": "snappy_native",
    "module_path": "./prebuilds/{platform}-{arch}",
    "host": "https://github.com/seu-usuario/snappy-js/releases/download/",
    "remote_path": "v{version}"
  },
  "files": [
    "dist/**/*",
    "deps/**/*",
    "binding.gyp",
    "README.md",
    "LICENSE",
    "prebuilds/**/*"
  ],
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "node-gyp": "^10.0.0",
    "prebuild": "^13.0.1",
    "prebuild-install": "^7.1.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@4.9.4+sha512.7b1cb0b62abba6a537b3a2ce00811a843bea02bcf53138581a6ae5b1bf563f734872bd47de49ce32a9ca9dcaff995aa789577ffb16811da7c603dcf69e73750b"
}
